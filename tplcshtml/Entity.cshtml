using System;
using @Model.nameSpace@(".DBM");
namespace @Model.nameSpace@(".Entity")
{
    /// <summary>
    /// 数据表: @Model.tableName
    /// </summary>
    public class @Model.className : BaseModel
    {
@foreach (Dictionary<string,string> item in Model.columns)
{
  string fieldName = item["fieldName"];
  if (fieldName.ToLower() == "id") continue;
  if (fieldName.ToLower() == "onff") continue;
    @("        ///") <summary>
    @("    ///") @item["comment"]
    @("    ///") </summary>
    @("        public ") @(item["fieldType"]) @(" " + item["fieldName"]) @(";\r\n\r\n") 
}
    }
}